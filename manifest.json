{
  "manifest_version": 2,
  "name": "Create Ticket Title",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "128": "icon_128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
          {
               "matches": ["https://login.bizmanager.yahoo.co.jp/login?url=https://promotionalads.business.yahoo.co.jp*"],
               "js": ["script.js"]
          }
     ],
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "permissions": [
    "tabs"
  ]
}
